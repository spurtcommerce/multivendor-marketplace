        "aws-sdk": "^2.391.0",
        "bcrypt": "^5.0.0",
        "chalk": "^2.4.1",
        "class-transformer": "^0.3.1",
        "class-transformer-validator": "^0.9.1",
        "class-validator": "^0.13.2",
        "commander": "^2.19.0",
        "compression": "^1.7.1",
        "convert-html-to-pdf": "^1.0.1",
        "copyfiles": "^2.1.0",
        "cors": "^2.8.5",
        "crypto": "^1.0.1",
        "crypto-js": "^4.0.0",
        "csvtojson": "^2.0.10",
        "dataloader": "^1.3.0",
        "dotenv": "6.0.0",
        "ejs": "^3.1.7",
        "event-dispatch": "^0.4.1",
        "exceljs": "^4.2.0",
        "express": "^4.16.2",
        "express-basic-auth": "^1.1.3",
        "express-ejs-layouts": "^2.5.0",
        "express-flash": "0.0.2",
        "express-graphql": "^0.12.0",
        "express-mysql-session": "^2.1.4",
        "express-session": "^1.16.2",
        "express-status-monitor": "^1.2.8",
        "express-useragent": "^1.0.15",
        "express-validator": "^5.3.1",
        "extract-zip": "^1.7.0",
        "faker": "^4.1.0",
        "fast-glob": "^3.2.11",
        "figlet": "^1.2.0",
        "findit": "^2.0.0",
        "fs": "0.0.1-security",
        "glob": "^7.1.2",
        "gm": "^1.23.1",
        "graphql": "15.8.0",
