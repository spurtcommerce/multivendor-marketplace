        "nps-utils": "^1.7.0",
        "parse-int": "^1.0.3",
        "passport": "^0.4.0",
        "passport-local": "^1.0.0",
        "paypal-rest-sdk": "^1.8.1",
        "puppeteer": "^13.3.2",
        "randomatic": "^3.1.1",
        "razorpay": "^2.0.6",
        "read-excel-file": "^5.2.28",
        "reflect-metadata": "^0.1.10",
        "request-ip": "^2.1.3",
        "rimraf": "^3.0.2",
        "routing-controllers": "^0.9.0",
        "serve-favicon": "^2.4.5",
        "simple-excel-to-json": "^2.2.1",
        "sitemap": "^7.1.1",
        "slugify": "^1.3.4",
        "stripe": "^8.15.0",
        "supertest": "^3.0.0",
        "swagger-ui-express": "^4.3.0",
        "to-words": "^2.2.5",
        "ts-node": "7.0.1",
        "tslint": "^5.8.0",
        "type-graphql": "^1.1.1",
        "typedi": "0.8.0",
        "typeorm": "^0.2.45",
        "typeorm-seeding": "^1.0.0-beta.6",
        "typeorm-typedi-extensions": "^0.2.1",
        "typescript": "^4.5.5",
        "uuid": "^3.3.2",
